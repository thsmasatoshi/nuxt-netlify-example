{
  "name": "nuxt-netlify-example",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "thsmasatoshi <ths.m.masatoshi@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "generatemd": "processmd 'contents/**/*.{yml,md}' --outputDir posts/json --watch",
    "summary": "processmd contents/**/*.md --stdout --outputDir posts/json > summary.json"
  },
  "dependencies": {
    "nuxt": "^1.0.0",
    "processmd": "^4.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
